<section class="section section--mb-s section--page page section--{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="page-custom">


  <!-- content section -->
    <div class="section"> 
      <div class="container container--tiny">
        <div class="section__title section__title--center">
          <h1 class="section__title-text h2">{{ page.title }}</h1>
        </div>
        <div class="page__content rte">
          {{ page.content }}
        </div>
      </div>
    </div>
  <!-- /content section -->

  <!-- one product 7 solutions-->
  <div id="prof_use_global" class="py-5 my-3" style="width:100%;background:green;">
    <div class="container">
        <div class="row">
        
            <div class="col-md-6">
              <div class="card" style="width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title">Card title</h5>
                  <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
                  <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                  <a href="#" class="card-link">Card link</a>
                  <a href="#" class="card-link">Another link</a>
                </div>
              </div>
            </div><!-- /col -->

            <div class="col-md-6">
              <div class="card" style="width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title">Card title</h5>
                  <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
                  <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
